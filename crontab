# m h  dom mon dow   command
1 1 * * * /bin/bash /root/denyip.sh
1 2 * * 0 /usr/bin/find /root/mnt/ -mtime +30 -name "log.ipv*" -exec rm -f {} \; 
1 3 1 * * echo  > /root/mnt/log.perf && echo > /root/mnt/log.keepalive
1 4 10 * * /bin/bash /root/topology/refreshGeoLite.sh
0 * * * * sleep `cat /root/mnt/second` && /bin/bash /root/mnt/perf.sh >> /root/mnt/log.perf 2>&1
*/5 * * * * /usr/bin/python /root/mnt/keepalive.py >> /root/mnt/log.keepalive 2>&1
@reboot rm -f /root/mnt/hour && rm -f /root/mnt/run && /bin/bash /root/mnt/perf.sh >> /root/mnt/log.perf 2>&1
@reboot /usr/sbin/ntpdate -u s1a.time.edu.cn
